<div class="container">
  <h1>New review</h1>

  <%= simple_form_for([@restaurant, @review]) do |f| %>
    <%= f.input :content %>
    <%= f.input :rating %>
    <%= link_to "Back", restaurants_path, class: "btn btn-success" %>
    <%= f.submit "Submit", class: "btn btn-primary" %>
  <% end %>
</div>
